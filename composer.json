{
    "name": "silverleague/logviewer",
    "description": "View your SilverStripe logs from inside the CMS",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/silverleague/silverstripe-logviewer",
    "license": "MIT",
    "keywords": ["silverstripe", "error", "cms", "log", "monolog"],
    "authors": [
        {
            "name": "Robbie Averill",
            "email": "robbie@silverstripe.com",
            "role": "Maintainer"
        }
    ],
    "require": {    
        "silverstripe/framework": "^5",
        "monolog/monolog": "~1.11",
        "silverstripe/crontask": "^3"
    },
    "require-dev": {        
        "squizlabs/php_codesniffer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "SilverLeague\\LogViewer\\": "src/",
            "SilverLeague\\LogViewer\\Tests\\": "tests/"
        }
    },
    "extra": {       
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
